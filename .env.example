# Stripe Configuration
EXPO_PUBLIC_STRIPE_PUBLISHABLE_KEY=your_stripe_publishable_key
EXPO_PUBLIC_STRIPE_SECRET_KEY=your_stripe_secret_key

# API Configuration
EXPO_PUBLIC_API_URL=your_api_url
EXPO_PUBLIC_API_VERSION=v1

# Feature Flags
EXPO_PUBLIC_ENABLE_ANALYTICS=true
EXPO_PUBLIC_ENABLE_CRASH_REPORTING=true
EXPO_PUBLIC_ENABLE_PUSH_NOTIFICATIONS=true

# Monitoring Configuration
EXPO_PUBLIC_ENABLE_MONITORING=true
EXPO_PUBLIC_ENABLE_REAL_TIME_MONITORING=true
EXPO_PUBLIC_ENABLE_PERFORMANCE_TESTS=false
EXPO_PUBLIC_MONITORING_WEBSOCKET_URL=ws://localhost:8080
EXPO_PUBLIC_MONITORING_ENVIRONMENT=development
EXPO_PUBLIC_MONITORING_LOG_LEVEL=info
EXPO_PUBLIC_MONITORING_ALERT_THROTTLE_MS=30000
EXPO_PUBLIC_MONITORING_METRICS_INTERVAL=5000
EXPO_PUBLIC_MONITORING_ALERTS_INTERVAL=10000

